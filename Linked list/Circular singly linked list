class node{
    int data;
    node next;
    node(int d){
        data=d;
        next=null;
    }
}

class ll{
    node head=null;
    node old=null;
    void ins(int val){
        node nn=new node(val);
        if(head==null){
            head=nn;
            nn.next=head;
            old=nn;
        }
        else{
            old.next=nn;
            nn.next=head;
            old=nn;
        }
    }
    void display(){
        if(head==null){
            return;
        }
        System.out.print(head.data+" ");
        node temp=head.next;
        while(temp!=head){
            System.out.print(temp.data+" ");
            temp=temp.next;
        }
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner z=new Scanner(System.in);
        ll l=new ll();
        while(z.hasNext()){
            int d=z.nextInt();
            l.ins(d);
        }
        l.display();
    }
}class node{
    int data;
    node next;
    node(int d){
        data=d;
        next=null;
    }
}

class ll{
    node head=null;
    node old=null;
    void ins(int val){
        node nn=new node(val);
        if(head==null){
            head=nn;
            nn.next=head;
            old=nn;
        }
        else{
            old.next=nn;
            nn.next=head;
            old=nn;
        }
    }
    void display(){
        if(head==null){
            return;
        }
        System.out.print(head.data+" ");
        node temp=head.next;
        while(temp!=head){
            System.out.print(temp.data+" ");
            temp=temp.next;
        }
    }
}
