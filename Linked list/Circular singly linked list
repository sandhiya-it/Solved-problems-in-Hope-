    public static void main(String[] args) {
        Scanner z=new Scanner(System.in);
        ll l=new ll();
        while(z.hasNext()){
            int d=z.nextInt();
            l.ins(d);
        }
        l.display();
    }
}   it is the slight modification of the singly linked list
